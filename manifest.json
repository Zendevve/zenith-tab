{
  "manifest_version": 3,
  "name": "Zenith Tab",
  "version": "1.1.0",
  "description": "A minimalist, customizable new tab page for focus and inspiration.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "geolocation"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "https://images.unsplash.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}